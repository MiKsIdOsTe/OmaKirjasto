CREATE TABLE "MANGA" (ID INTEGER NOT NULL PRIMARY KEY GENERATED ALWAYS AS IDENTITY(START WITH 1, INCREMENT BY 1), NIMI VARCHAR (50), TEKIJA VARCHAR (100), KUSTANTAJA VARCHAR (50), KIELI VARCHAR (20));

CREATE TABLE "MGKIRJASTO" (KIRJASTOID INTEGER NOT NULL PRIMARY KEY GENERATED ALWAYS AS IDENTITY(START WITH 1, INCREMENT BY 1), ID_MANGA INT, NRO VARCHAR(50));
